{
  "name": "@jungle-gaming/api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for Jungle Gaming Task Management",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 50",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jungle-gaming/types": "workspace:*",
    "@jungle-gaming/utils": "workspace:*",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.1",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.0.1",
    "amqp-connection-manager": "^4.1.14",
    "axios": "^1.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express-rate-limit": "^7.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@jungle-gaming/tsconfig": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.0.0",
    "@types/node": "^20.14.0",
    "@types/passport-jwt": "^3.0.13",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^8.57.0",
    "globals": "^16.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.44.0"
  }
}